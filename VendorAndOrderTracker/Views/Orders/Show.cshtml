@{
  Layout = "_Layout";
}

<h1>Order Details: </h1>

<p>@Model["orders"].Title</p>
<p>@Model["orders"].Description</p>
<p>@Model["orders"].Price</p>
<p>@Model["orders"].Date</p>
<h3>Vendor: @Model["vendor"].Name</h3>

<a href='/vendors/@Model["vendor"].VendorId/orders/new'>Add another order</a>
<a href='/vendors'>View all vendors</a>
