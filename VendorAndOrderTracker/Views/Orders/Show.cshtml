@{
  Layout = "_Layout";
}

<h2>Order Details: </h2>
<h3>Vendor: @Model["vendor"].Name</h3>
<h>Title: @Model["orders"].Title</h><br>
<h>Description: @Model["orders"].Description</h><br>
<h>Price: @Model["orders"].Price$</h><br>
<h>Date: @Model["orders"].Date</h><br>


<a href='/vendors/@Model["vendor"].VendorId/orders/new'>Add another order</a><br>
<a href='/vendors'>View all vendors</a>
